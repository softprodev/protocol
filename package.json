    "test-ether-token-user-interactions": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 10000 --recursive test/DmmEther.UserInteractions.test.js --slow 2500",
    "test-token-contract-interactions": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 10000 --recursive test/DmmToken.ContractInteractions.test.js --slow 2500",
    "test-token-gasless-interactions": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 10000 --recursive test/DmmToken.Gasless*.test.js --slow 2500",
    "test-token-user-interactions": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 10000 --recursive test/DmmToken.UserInteractions.test.js --slow 2500",
    "test-controller": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 10000 --recursive test/DmmController.test.js --slow 2500"
