{
  "name": "dmm-protocol",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "oz compile",
    "deploy": "oz compile && ENVIRONMENT=LOCAL node scripts/initial_deployment/index.js",
    "deploy-mainnet": "oz compile && ENVIRONMENT=PRODUCTION node scripts/initial_deployment/index.js",
    "deploy-mainnet-off-chain-asset-valuator": "oz compile && ENVIRONMENT=PRODUCTION node scripts/deploy_off_chain_asset_valuator/DeployOffChainAssetValuator.js",
    "deploy-testnet": "oz compile && ENVIRONMENT=TESTNET DEPLOYER=1234567812345678123456781234567812345678123456781234567812345678 node scripts/initial_deployment/index.js",
    "encode-abi": "DEPLOYER=1234567812345678123456781234567812345678123456781234567812345678 node scripts/EncodeAbi.js",
    "format": "solium -d contracts/",
    "run-playground": "DEPLOYER=1234567812345678123456781234567812345678123456781234567812345678 node ./scripts/Playground.js",
    "send-ether": "ENVIRONMENT=LOCAL RECIPIENT=0x8D7f03FdE1A626223364E592740a233b72395235 node ./scripts/SendEther.js",
    "start-chain": "ganache-cli --deterministic --port 8545 -l 10000000 -i 1337",
    "test": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/ --slow 2500",
    "test-controller": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/DmmController*.test.js --slow 2500",
    "test-controller-admin-reserves": "oz compile && npx mocha --exit --async-only --timeout 15000 --recursive test/DmmController.AdminReserves.test.js --slow 2500",
    "test-controller-collateralization": "oz compile && npx mocha --exit --async-only --timeout 15000 --recursive test/DmmController.Collateralization.test.js --slow 2500",
    "test-controller-misc": "oz compile && npx mocha --exit --async-only --timeout 15000 --recursive test/DmmController.Misc.test.js --slow 2500",
    "test-controller-transfer-ownership": "oz compile && npx mocha --exit --async-only --timeout 15000 --recursive test/DmmController.TransferOwnership.test.js --slow 2500",
    "test-ether-token": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/DmmEther*.test.js --slow 2500",
    "test-ether-token-user-interactions": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/DmmEther.UserInteractions.test.js --slow 2500",
    "test-ether-token-gasless-interactions": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/DmmEther.Gasless*.test.js --slow 2500",
    "test-interest-rate-v1": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/InterestRateImplV1.test.js --slow 2500",
    "test-token": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/DmmToken*.test.js --slow 2500",
    "test-token-gasless-interactions": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/DmmToken.Gasless*.test.js --slow 2500",
    "test-token-user-interactions": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/DmmToken.UserInteractions.test.js --slow 2500",
    "test-underlying-token-valuator-v1": "oz compile && npx mocha-parallel-tests --exit --async-only --timeout 15000 --recursive test/UnderlyingTokenValuatorImplV1.test.js --slow 2500"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/cli": "^2.7.1",
    "@openzeppelin/contract-loader": "^0.6.1",
    "@openzeppelin/contracts": "^2.4.0",
    "chainlink": "^0.8.2",
    "ganache-cli": "^6.9.0"
  },
  "devDependencies": {
    "@openzeppelin/test-environment": "^0.1.2",
    "@openzeppelin/test-helpers": "^0.5.4",
    "chai": "^4.2.0",
    "ethereumjs-util": "latest",
    "mocha": "^7.0.1",
    "mocha-parallel-tests": "^2.3.0"
  }
}
